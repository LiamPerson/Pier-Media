		"@types/got": "^9.6.12",
		"@types/jest": "^29.5.8",
		"@types/mime-types": "^2.1.1",
		"concurrently": "^8.2.0",
		"eslint-plugin-unused-imports": "^3.0.0",
		"eslint-plugin-validate-jsx-nesting": "^0.1.1",
		"husky": "^8.0.0",
		"jest": "^29.7.0",
		"jest-mock-extended": "^3.0.5",
