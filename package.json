{
	"name": "pier-media-server",
	"scripts": {
		"dev": "concurrently \"next dev\" \"npm run codegen:watch\"",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"prepare": "husky install",
		"codegen": "graphql-codegen --config gql/config.ts",
		"codegen:watch": "graphql-codegen --config gql/config.ts --watch"
	},
	"dependencies": {
		"@apollo/client": "^3.7.17",
		"@apollo/server": "^4.9.1",
		"@as-integrations/next": "^2.0.1",
		"@emotion/react": "^11.11.1",
		"@emotion/styled": "^11.11.0",
		"@mui/icons-material": "^5.14.1",
		"@mui/material": "^5.14.1",
		"@prisma/client": "^5.1.1",
		"@tanstack/react-query": "^4.32.6",
		"@types/node": "20.4.2",
		"@types/react": "18.2.15",
		"@types/react-dom": "18.2.7",
		"ajv": "^8.12.0",
		"axios": "^1.4.0",
		"eslint": "8.45.0",
		"eslint-config-next": "13.4.10",
		"graphql": "^16.7.1",
		"next": "13.4.10",
		"prisma": "^5.1.1",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"typescript": "5.1.6",
		"usehooks-ts": "^2.9.1"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "5.0.0",
		"@graphql-codegen/client-preset": "^4.1.0",
		"@graphql-codegen/typescript": "^4.0.1",
		"@graphql-codegen/typescript-resolvers": "^4.0.1",
		"@parcel/watcher": "^2.2.0",
		"concurrently": "^8.2.0",
		"husky": "^8.0.0",
		"lint-staged": "^13.2.3",
		"prettier": "^3.0.0"
	},
	"lint-staged": {
		"*.{js,ts,tsx,json,graphql}": [
			"prettier --write"
		]
	}
}
