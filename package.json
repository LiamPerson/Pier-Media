		"@prisma/client": "^5",
		"@types/node": "^20",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"deepmerge-ts": "^5",
		"eslint": "^8",
		"eslint-config-next": "^13",
		"file-type": "^18",
		"got": "^13",
		"graphql": "^16",
		"image-size": "^1",
		"mime-types": "^2",
		"music-metadata": "^8",
		"next": "^13",
		"prisma": "^5",
		"react": "^18",
		"react-dom": "^18",
