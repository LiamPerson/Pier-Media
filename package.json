		"@emotion/styled": "^11.11.0",
		"@mui/icons-material": "^5.14.1",
		"@mui/material": "^5.14.1",
		"@prisma/client": "^5.1.1",
		"@types/node": "20.4.2",
		"@types/react": "18.2.15",
		"@types/react-dom": "18.2.7",
		"deepmerge-ts": "^5.1.0",
		"eslint": "8.45.0",
		"eslint-config-next": "13.4.10",
		"file-type": "^18.5.0",
		"got": "^13.0.0",
		"graphql": "^16.7.1",
		"image-size": "^1.0.2",
		"mime-types": "^2.1.35",
		"music-metadata": "^8.1.4",
		"next": "13.4.10",
